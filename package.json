{
	"name": "rust-doc-viewer",
	"displayName": "Rust Doc Viewer",
	"description": "View locally generated rust documentation within VS Code",
	"author": {
		"name": "Jacob Scearcy",
		"email": "ugliestlemming@gmail.com",
		"url": "http://js-consults.com/"
	},
	"publisher": "JScearcy",
	"repository": {
		"url": "https://github.com/JScearcy/rust-doc-viewer",
		"type": "git"
	},
	"icon": "images/rust-cubes.png",
	"galleryBanner": {
		"color": "#0e95b7",
		"theme": "dark"
	},
	"version": "1.0.11",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.rustDocViewer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.rustDocViewer",
				"title": "Rust: Doc Viewer"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.33",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
